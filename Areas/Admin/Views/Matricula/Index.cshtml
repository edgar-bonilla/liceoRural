@model IEnumerable<LICEORURALJASMINEZB.Models.Matricula>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NivelDetalleCurso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alumno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NivelDetalle)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NivelDetalleCurso.IdNivelDetalleCurso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Alumno.Apellidos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NivelDetalle.IdNivelDetalle)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMatricula">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMatricula">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMatricula">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
