@model LICEORURALJASMINEZB.Models.Alumno

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<div class="container">
    <div >
        <div >
            <h3><strong>Detalle Alumno</strong></h3>
        </div>
    </div>
    <br />
    <div class="border backgroundWhite">

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Nombres" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Nombres" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Apellidos" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Apellidos" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="DocumentoIdentidad" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="DocumentoIdentidad" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="FechaNacimiento" class="col-form-label"></label>
            </div>
            <div class="col-5">
                @Html.DisplayFor(m => Model.FechaNacimiento)
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Sexo" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Sexo" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Ciudad" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Ciudad" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Direccion" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Direccion" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Correo" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Correo" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Telefono" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Telefono" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Estado" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Estado" disabled class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="FechaRegistro" class="col-form-label"></label>
            </div>
            <div class="col-5">
                @Html.DisplayFor(m => Model.FechaRegistro)
            </div>
        </div>
        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_DetailsAndBackToListButton" model="Model.Id" />
            </div>
        </div>

    </div>

    @section Scripts{
        @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
    }

</div>